{
  "name": "nemo-automation",
  "version": "1.0.0",
  "description": "Nemojs for e2e tests",
  "license": "MIT",
  "scripts": {
    "node_clean": "rimraf node_modules",
    "lint": "./node_modules/.bin/eslint .",
    "nemo": "node_modules/.bin/nemo-runner -B tests/functional -P chrome -G @some",
    "nemo:ff": "node_modules/.bin/nemo-runner -B tests/functional -P firefox -G @some",
    "nemo:debug": "set DEBUG=nemo* nemo-runner -B tests/functional -P chrome -G @some",
    "report": "allure generate allure-results --clean -o allure-report && allure open allure-report"
  },
  "devDependencies": {
    "allure-commandline": "^2.4.1",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-string": "^1.4.0",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "flow-bin": "0.61.0",
    "mocha": "^3.1.2",
    "mocha-allure-reporter": "^1.3.2",
    "mochawesome": "^3.0.0",
    "nemo": "^3.0.0",
    "nemo-runner": "^1.3.0",
    "nemo-screenshot": "^3.0.0",
    "nemo-view": "^3.0.0",
    "webpack": "^3.10.0"
  }
}
