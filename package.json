{
  "name": "nemo-automation",
  "version": "1.0.0",
  "description": "Nemojs for e2e tests",
  "scripts": {
    "clean": "rimraf node_modules",
    "win_clean": "rmdir /S /Q node_modules",
    "lint": "./node_modules/.bin/eslint .",
    "nemo": "node_modules/.bin/nemo-runner -B tests/functional -P firefox -G @some",
    "nemo:debug": "set DEBUG=nemo* nemo-runner -B tests/functional -P chrome -G @some",
    "mocha": "./node_modules/.bin/_mocha tests/functional/spec/*.js"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "flow-bin": "^0.40.0",
    "mocha": "^3.1.2",
    "chai": "^3.5.0",
    "mochawesome": "^3.0.0",
    "nemo": "^3.0.0",
    "nemo-runner": "^1.3.0",
    "nemo-screenshot": "^3.0.0",
    "nemo-view": "^3.0.0"
  }
}
